{
    "name": "lang-html-server",
    "version": "1.0.0",
    "description": "Simple server to run lang.html",
    "main": "lang.html",
    "scripts": {
      "start": "npx http-server . -p 3000 -o lang.html",
      "serve": "npx http-server . -p 8080 -o lang.html",
      "run": "npx live-server --port=3000 --open=lang.html"
    },
    "keywords": ["html", "server", "static"],
    "author": "",
    "license": "MIT",
    "devDependencies": {
      "http-server": "^14.1.1",
      "live-server": "^1.2.2"
    }
  }